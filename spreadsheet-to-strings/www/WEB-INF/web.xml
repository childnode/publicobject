<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app
   xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5">
  <display-name>Spreadsheet to Strings</display-name>

  <servlet>
    <servlet-name>showxml</servlet-name>
    <servlet-class>com.publicobject.ss2s.web.ShowXmlServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>key</servlet-name>
    <servlet-class>com.publicobject.ss2s.web.KeyServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>showxml</servlet-name>
    <url-pattern>/ShowXml</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>key</servlet-name>
    <url-pattern>/Key/*</url-pattern>
  </servlet-mapping>
</web-app>
